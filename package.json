{
	"name": "file-info-viewer",
	"displayName": "File Info Viewer",
	"description": "Vscode extension to display file information for specified file types",
	"version": "1.0.1",
	"license": "MIT",
	"publisher": "wxmvv",
	"icon": "icon/icon.png",
	"homepage": "https://github.com/wxmvv/file-info-viewer",
	"repository": {
		"type": "git",
		"url": "https://github.com/wxmvv/file-info-viewer"
	},
	"engines": {
		"vscode": "^1.87.0"
	},
	"keywords": [
		"file",
		"properties",
		"size",
		"date",
		"media info",
		"mime",
		"creation date",
		"modification date",
		"access date"
	],
	"main": "./dist/extension.js",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:file-info-viewer.fileInfoViewer"
	],
	"contributes": {
		"commands": [],
		"customEditors": [
			{
				"viewType": "file-info-viewer.fileInfoViewer",
				"displayName": "File Info Viewer",
				"selector": [
					{
						"filenamePattern": "*"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.87.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"eslint": "^9.25.1",
		"typescript": "^5.8.3",
		"ts-loader": "^9.5.2",
		"webpack": "^5.99.7",
		"webpack-cli": "^6.0.1",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2"
	}
}
